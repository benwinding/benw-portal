{
  "name": "benw-portal",
  "version": "2.0.0",
  "description": "My Portal!",
  "author": "Ben Winding <ben.winding@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=\"--max_old_space_size=4096 --inspect\" next dev",
    "clean": "rimraf dist",
    "start": "next dev",
    "test": "yarn jest",
    "build": "rm -rf dist && next build",
    "build-3d": "yarn webpack -c ./public/3d/webpack.config.js",
    "build-sitemap": "next-sitemap --config ./next-sitemap.config.mjs",
    "deploy": "dokku-pages deploy -g dokku@benwinding.com:portal -d dist --minimal --allow-cors '.+\\.benwinding.com|localhost:.+'",
    "lint": "dprint fmt"
  },
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@react-three/drei": "^7.9.0",
    "@react-three/fiber": "^7.0.7",
    "@types/react-transition-group": "^4.4.4",
    "autoprefixer": "^10.4.2",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "color-interpolate": "^1.0.2",
    "dayjs": "^1.11.10",
    "dokku-pages": "^1.0.12",
    "dprint": "^0.35.3",
    "eslint": "^5.16.0",
    "eslint-config-next": "^13.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "github-markdown-css": "^5.5.1",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "^13.3.0",
    "next-sitemap": "^3.0.2",
    "postcss": "^8.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "4.4.5",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "tailwindcss": "^3.2.4",
    "three": "^0.132.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^17.0.35",
    "@types/react": "18.2.72",
    "@types/three": "^0.143.1",
    "jest": "29.7.0",
    "svg-inline-loader": "^0.8.2",
    "svgo-loader": "^3.0.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
